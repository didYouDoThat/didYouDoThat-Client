{
  "expo": {
    "name": "jisoook",
    "scheme": "com.jisoook.didYouDoThat",
    "slug": "didYouDoThat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-cat.png",
    "splash": {
      "image": "./assets/splash-cat.png",
      "resizeMode": "contain",
      "backgroundColor": "#a6dcef"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jisoook.didYouDoThat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-cat.png",
        "backgroundColor": "#a6dcef"
      },
      "package": "com.jisoook.didYouDoThat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
